{
    "name": "riotcoke/laravel-kick-oauth",
    "description": "Kick OAuth2 integration for Laravel with auto-refreshing tokens",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "RIoTCOKE",
            "email": "RIOTCOKE123@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
